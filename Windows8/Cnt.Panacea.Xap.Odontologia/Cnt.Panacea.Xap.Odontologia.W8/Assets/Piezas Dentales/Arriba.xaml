			<Path.Fill>
                <ImageBrush Stretch="Fill" ImageSource="ms-appx:///Images/59229.png"/>
			</Path.Fill>
		</Path>
		<TextBlock x:Name="texto" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="9.333" Margin="8,-5,6,4" Height="10"/>
	</Grid>
