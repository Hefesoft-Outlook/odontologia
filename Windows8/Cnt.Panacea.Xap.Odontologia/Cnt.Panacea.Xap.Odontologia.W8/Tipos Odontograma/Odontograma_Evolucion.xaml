<UserControl
    x:Class="App2.Tipos_Odontograma.Odontograma_Evolucion"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:App2.Tipos_Odontograma"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    DataContext="{Binding Evolucion, Source={StaticResource Locator}}"
    mc:Ignorable="d"
    d:DesignHeight="50"
    d:DesignWidth="900">
    
    <Grid>
        <StackPanel>
            <HyperlinkButton Content="Usuario abandono" Command="{Binding usuarioAbandonoCommand}"/>
        </StackPanel>
    </Grid>
</UserControl>
