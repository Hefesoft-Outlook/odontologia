<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:Mapa_Dental="clr-namespace:Cnt.Panacea.Xap.Odontologia.Assets.Mapa_Dental" 
    xmlns:Assets="clr-namespace:Cnt.Panacea.Xap.Odontologia.Assets" 
    x:Class="Cnt.Panacea.Xap.Odontologia.Views.Odontogramav2"
    mc:Ignorable="d"
    d:DesignHeight="400" d:DesignWidth="900">
    
	<Grid x:Name="LayoutRoot" Background="White">
		<Grid.RowDefinitions>
			<RowDefinition Height="29*"/>
			<RowDefinition Height="121*"/>
		</Grid.RowDefinitions>

		<Mapa_Dental:Mapa_Pieza_Dentales x:Name="Mapa_Pieza_Dentales" Visibility="Visible" Grid.Row="1"/>

		<Assets:Paleta x:Name="Paleta" VerticalAlignment="Top"/>

	</Grid>
</UserControl>
