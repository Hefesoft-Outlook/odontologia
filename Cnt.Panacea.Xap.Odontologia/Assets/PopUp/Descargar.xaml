<controls:ChildWindow x:Class="Cnt.Panacea.Xap.Odontologia.PopUp.Descargar"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
           Width="400" Height="300" 
           Title="{Binding Mensajes.Descargar, Source={StaticResource publicResource}}">
    <Grid x:Name="LayoutRoot" Margin="2">
        
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Button x:Name="CancelButton" Content="{Binding Mensajes.Cancelar, Source={StaticResource publicResource}}" Click="CancelButton_Click" Width="75" Height="23" HorizontalAlignment="Right" Margin="0,12,0,0" Grid.Row="1" />
        <Button x:Name="OKButton" Content="{Binding Mensajes.Guardar_Equipo, Source={StaticResource publicResource}}" Click="OKButton_Click" Height="23" Margin="163,12,87,0" Grid.Row="1" />
        <Image Name="ImagenCargar" Stretch="Fill" />
    </Grid>
</controls:ChildWindow>